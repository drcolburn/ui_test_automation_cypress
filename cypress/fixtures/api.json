{
  "endpoints": {
    "login": "/api/auth/login",
    "logout": "/api/auth/logout",
    "users": "/api/users",
    "products": "/api/products",
    "orders": "/api/orders"
  },
  "apiResponses": {
    "successLogin": {
      "status": 200,
      "body": {
        "success": true,
        "token": "mock-jwt-token",
        "user": {
          "id": 1,
          "username": "testuser",
          "email": "testuser@example.com"
        }
      }
    },
    "failedLogin": {
      "status": 401,
      "body": {
        "success": false,
        "message": "Invalid credentials"
      }
    }
  }
}
